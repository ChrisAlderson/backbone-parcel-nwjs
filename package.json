{
  "name": "backbone-parcel-nwjs",
  "description": "Simple Parcel Backbone and NW.js app",
  "version": "1.0.0",
  "main": "dist/index.html",
  "scripts": {
    "build": "build --tasks win-x86,win-x64,linux-x86,linux-x64,mac-x64 --mirror https://dl.nwjs.io/ .",
    "lint": "standard src/**/*.js --parser babel-eslint",
    "parcel": "parcel src/index.html",
    "nwjs": "run --x64 --mirror https://dl.nwjs.io/ ."
  },
  "build": {
    "nwVersion": "lts"
  },
  "license": "MIT",
  "dependencies": {
    "backbone": "^1.3.3",
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.1.2",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "nwjs-builder-phoenix": "^1.14.6",
    "parcel-bundler": "^1.3.1",
    "standard": "^10.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisalderson/backbone-parcel-nwjs"
  }
}
